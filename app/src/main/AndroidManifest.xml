<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ichirotech.bratanata.mynoteapp">
    <permission android:name="com.ichirotech.bratanata.mynoteapp.READ_DATABASE" android:protectionLevel="normal"/>
    <permission android:name="com.ichirotech.bratanata.mynoteapp.WRITE_DATABASE" android:protectionLevel="normal"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FormAddUpdateActivity"></activity>
        <provider
            android:authorities="com.ichirotech.bratanata.mynoteapp"
            android:name=".Provider.NoteProvider"
            android:exported="true"
            android:readPermission="com.ichirotech.bratanata.mynoteapp.READ_DATABASE"
            android:writePermission="com.ichirotech.bratanata.mynoteapp.WRITE_DATABASE"/>
    </application>

</manifest>